import random
x = int(input("guess a number between 1-10"))
y = random.randint(1,10) 
if x != y :
    print("wrong")
else:
    print("right")

while x != y:   
    print( "try again")
    x = int(input("guess a number between 1-10"))
    if x != y :
        print("wrong")
    else:
        print("right")
